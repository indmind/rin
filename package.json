{
    "name": "rin",
    "version": "0.0.3",
    "description": "simple multi purpose bot",
    "main": "lib/main.js",
    "engines": {
        "node": "8.9.1",
        "npm": "5.7.1"
    },
    "scripts": {
        "build": "rm lib/ -rf && ./node_modules/.bin/babel src -d lib",
        "dev": "./node_modules/.bin/babel-watch src/main.js",
        "cli": "npm run build && STATUS=debug npm start",
        "start": "node lib/main.js",
        "lint": "./node_modules/.bin/eslint --fix .",
        "coala": "coala --non-interactive",
        "test": "jest",
        "heroku-postbuild": "npm run build"
    },
    "jest": {
        "coverageDirectory": "./coverage/",
        "collectCoverage": true
    },
    "author": "indmind",
    "license": "MIT",
    "dependencies": {
        "axios": "^0.18.0",
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0",
        "cssbeautify": "^0.3.1",
        "currency-formatter": "^1.4.2",
        "discord.js": "^11.3.2",
        "dotenv": "^5.0.1",
        "express": "^4.16.3",
        "figlet": "^1.2.0",
        "goo.gl": "^0.1.4",
        "google-translate-api": "^2.3.0",
        "is-json": "^2.0.1",
        "is-reachable": "^2.4.0",
        "jawaskrip": "^0.4.6",
        "mathjs": "^6.0.2",
        "moment": "^2.22.1",
        "money": "^0.2.0",
        "node-sass": "^4.9.0",
        "prettyjson": "^1.2.1",
        "rata": "0.0.1",
        "showdown": "^1.8.6",
        "simple-markdown": "^0.4.0",
        "telegraf": "^3.21.0",
