        "babel-cli": "^6.26.0",
        "babel-eslint": "^8.2.3",
        "babel-jest": "^23.0.1",
        "babel-plugin-wildcard": "^5.0.0",
        "babel-watch": "^2.0.7",
        "eslint": "^4.19.1",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-import": "^2.11.0",
        "eslint-plugin-jest": "^21.15.1",
        "eslint-plugin-prettier": "^2.6.0",
        "jest": "^22.4.3",
        "prettier": "^1.12.1",
        "regenerator-runtime": "^0.11.1"
